<button>
    <%= link_to("Home Page", root_path, :class => 'action index') %>
</button>

<table class="list" summary="Book">
    <tr class="header">
      <th>ID</th>
      <th>Title</th>
      <th>Author</th>
      <th>Genre</th>
      <th>Price</th>
      <th>Published</th>
    </tr>
    <tr>
      <td><%= @book.id %></td>
      <td><%= @book.title %></td>
      <td><%= @book.author %></td>
      <td><%= @book.genre %></td>
      <td><%= @book.price %></td>
      <td><%= @book.published %></td>
      
    </tr>
  </table>